public class Main
{
	public static void main(String[] args) {
	//Math	
		double x,y,z;
		x=90;
		y=100;
		System.out.println("x="+x);
		System.out.println("y="+y);
		z=Math.sin(x);
		//Math.sqrt(a);
		//Math.pow(a,b);
		System.out.println("sin x= "+z);
		// Strings
		String S = new String("333.111");
		System.out.println(S);
		System.out.println(S.substring(0, 3));
	String S1 = new String(S.substring(0, 3));
	System.out.println(S1);
		String S2 = new String(S.substring(4));
	System.out.println(S2);
		String S3 = new String(S.substring(3,4));
	System.out.println(S3);
	String Srevert= S2+S3+S1;
	System.out.println(Srevert);
		// calculate time in hh mm ss from seconds
	int timestart = 116;
	int timeM1 = timestart/60; 
	//multiply modulo to 10 - it is ss
	int times = timestart%60;
	System.out.println(times);
	System.out.println(timeM1);
	double timeh = timeM1/60; 
	int timem = timeM1%60;
//create string with seconds
	if (times<10) {
	    String secondoutput;
      secondoutput = String.format("0" + times);
      System.out.println(secondoutput);
	}
	else
	 	{ 
	 String secondoutput;
	   secondoutput = String.format("" + times);
	   System.out.println(secondoutput);
	    	}
	    	//create string with minutes
	    	
	if (timem<10) {
	    String minuteoutput;
      minuteoutput = String.format("0" + timem);
      System.out.println(minuteoutput);
	}
	else
	 	{ 
	 String minuteoutput;
	   minuteoutput = String.format("" + timem);
	   System.out.println(minuteoutput);
	    	}
	//create string with hh
	if (timeh==0) {
	    String houroutput1;
      houroutput1 = String.format("0" + "0");
      System.out.println(houroutput1);
	    
	} 
      else {
	if (timeh<10) {
	    String houroutput1;
      houroutput1 = String.format("0" + timeh);
      System.out.println(houroutput1);
	}
	else
	 	{ 
	 String houroutput1;
	   houroutput1 = String.format("" + timeh);
	   System.out.println(houroutput1);
	    	}
	}
	
	//System.out.println(houroutput1);
	//	System.out.println(timestart + " seconds is "  + "." + minuteoutput + "." + secondoutput);
	}

	}
}
